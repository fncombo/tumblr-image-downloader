{
  "name": "Tumblr Image Downloader",
  "version": "0.8.4",
  "description": "Download images from your Tumblr dashboard, like, archive, and search pages with a single click.",
  "permissions": [
    "storage",
    "notifications",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.tumblr.com/tagged/*",
        "http://*.tumblr.com/blog/*",
        "http://*.tumblr.com/dashboard",
        "http://*.tumblr.com/dashboard/*",
        "http://*.tumblr.com/likes",
        "http://*.tumblr.com/likes/*",
        "http://*.tumblr.com/archive",
        "http://*.tumblr.com/archive/*"
        ],
      "js": [
        "js/core.js",
        "js/script.js"
      ],
      "css": ["css/style.css"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "options_page": "html/options.html",
  "page_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_title": "Tumblr Image Downloader Options"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "manifest_version": 2
}
