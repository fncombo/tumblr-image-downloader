{
  "name": "Tumblr Image Downloader",
  "version": "0.7.1",
  "description": "Download images on your Tumblr dashboard, likes, and search pages with a single classy click.",
  "permissions": [
    "storage",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.tumblr.com/tagged/*",
        "http://*.tumblr.com/blog/*",
        "http://*.tumblr.com/dashboard",
        "http://*.tumblr.com/dashboard/*",
        "http://*.tumblr.com/likes",
        "http://*.tumblr.com/likes/*"
        ],
      "js": ["js/script.js"],
      "css": ["css/style.css"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "options_page": "html/options.html",
  "icons": {
    "16": "img/icon16.png",
    "128": "img/icon128.png"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "manifest_version": 2
}
