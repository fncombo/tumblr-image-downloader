{
  "name": "tumblr image downloader",
  "version": "0.5.4",
  "description": "Download images on tumblr /tagged/ pages with a single classy click.",
  "permissions": [
    "storage",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.tumblr.com/tagged/*"],
      "js": ["script.js"],
      "css": ["style.css"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
