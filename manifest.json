{
  "name": "Tumblr Image Downloader",
  "version": "0.5.6",
  "description": "Download images on Tumblr /tagged/ pages with a single classy click.",
  "permissions": [
    "storage",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.tumblr.com/tagged/*"],
      "js": ["js/script.js"],
      "css": ["css/style.css"],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "options_page": "html/options.html",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2
}
