{
  "name" : "tumblr image saver",
  "version" : "0.1",
  "description" : "Download images on tumblr /tagged/ pages with a single classy click.",
  "content_scripts" : [
    {
      "matches" : [ "http://*.tumblr.com/tagged/*" ],
      "js" : [ "script.js" ],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "icons" : {
    "16" : "icon16.png",
    "48" : "icon48.png",
    "128" : "icon128.png"
  },
  "manifest_version": 2
}
